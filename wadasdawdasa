-- ==================== PERMANENT ESP (kapag ON lang) ====================
local function addESP(plr)
    if plr == LocalPlayer then return end
    plr.CharacterAdded:Connect(function(char)
        repeat task.wait() until char and char:FindFirstChild("Head")
        task.wait(0.3)
        if not espEnabled then return end
        if char:FindFirstChildOfClass("Highlight") then return end
        
        local hl = Instance.new("Highlight")
        hl.Name = "MONSY_ESP"
        hl.Parent = char
        hl.Adornee = char
        hl.FillTransparency = 1
        hl.OutlineColor = Color3.new(1,1,1)
        hl.OutlineTransparency = 0
        hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    end)
end

-- Kapag nag-off/on ng ESP
spawn(function()
    while task.wait(0.5) do
        for _, plr in Players:GetPlayers() do
            if plr ~= LocalPlayer and plr.Character then
                local hl = plr.Character:FindFirstChild("MONSY_ESP")
                if espEnabled then
                    if not hl then
                        local newhl = Instance.new("Highlight")
                        newhl.Name = "MONSY_ESP"
                        newhl.Parent = plr.Character
                        newhl.Adornee = plr.Character
                        newhl.FillTransparency = 1
                        newhl.OutlineColor = Color3.new(1,1,1)
                        newhl.OutlineTransparency = 0
                        newhl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                    end
                else
                    if hl then hl:Destroy() end
                end
            end
        end
    end
end)

for _, plr in Players:GetPlayers() do addESP(plr) end
Players.PlayerAdded:Connect(addESP)

-- ==================== FOV & Aimbot (same super fast) ====================
local fovCircle = Drawing.new("Circle")
fovCircle.Thickness = 2.5
fovCircle.Color = Color3.new(1,1,1)
fovCircle.Filled = false
fovCircle.Radius = fovRadius
fovCircle.NumSides = 100
fovCircle.Transparency = 1
fovCircle.Visible = true

local function getClosest()
    local closest, shortest = nil, math.huge
    local mouse = UIS:GetMouseLocation()
    for _, plr in Players:GetPlayers() do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("Head") then
            local head = plr.Character.Head
            local hum = plr.Character:FindFirstChildOfClass("Humanoid")
            if hum and hum.Health > 0 then
                local pos, onScreen = Camera:WorldToViewportPoint(head.Position)
                if onScreen then
                    local dist = (Vector2.new(pos.X, pos.Y) - mouse).Magnitude
                    if dist < shortest and dist <= fovRadius then
                        shortest = dist
                        closest = head
                    end
                end
            end
        end
    end
    return closest
end

local function aimAt(t)
    local pos = Camera:WorldToViewportPoint(t.Position)
    local mouse = UIS:GetMouseLocation()
    local dx, dy = pos.X - mouse.X, pos.Y - mouse.Y
    mousemoverel(dx, dy)
    mousemoverel(dx*0.3, dy*0.3)
end

UIS.InputBegan:Connect(function(i,gp)
    if not gp and i.UserInputType == Enum.UserInputType.MouseButton2 then aiming = true end
end)
UIS.InputEnded:Connect(function(i,gp)
    if not gp and i.UserInputType == Enum.UserInputType.MouseButton2 then aiming = false end
end)

RunService.RenderStepped:Connect(function()
    fovCircle.Position = UIS:GetMouseLocation()
    fovCircle.Radius = fovRadius
    fovCircle.Visible = fovVisible
    
    if aiming and aimbotEnabled then
        local target = getClosest()
        if target then aimAt(target) end
    end
end)
            ]])()
